<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".edit_profile_activity">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="152dp"
        android:layout_height="22dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="Edit Profile"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="26dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036"
        app:srcCompat="@drawable/baseline_arrow_back_ios_24" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="120dp"
        android:layout_height="110dp"
        app:srcCompat="@drawable/baseline_account_circle_24"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="55dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="27dp"
        android:layout_height="28dp"
        android:fontFamily="@font/inter_medium"
        android:text="Edit"
        tools:layout_editor_absoluteX="281dp"
        tools:layout_editor_absoluteY="120dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:srcCompat="@drawable/baseline_create_24"
        tools:layout_editor_absoluteX="308dp"
        tools:layout_editor_absoluteY="120dp" />


    <TextView
        android:id="@+id/textView22"
        android:layout_width="107dp"
        android:layout_height="22dp"
        android:fontFamily="@font/archivo_black"
        android:text="User Name"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="190dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="343dp"
        android:layout_height="50dp"
        android:background="@drawable/edittext_background"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColor="#FDFDFD"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="212dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="148dp"
        android:layout_height="22dp"
        android:fontFamily="@font/archivo_black"
        android:text="Phone Number"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="277dp" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="343dp"
        android:layout_height="50dp"
        android:background="@drawable/edittext_background"
        android:ems="10"
        android:hint="Enter Your Number"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColor="#FDFDFD"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="299dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="58dp"
        android:layout_height="22dp"
        android:fontFamily="@font/archivo_black"
        android:text="Email"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="363dp" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="343dp"
        android:layout_height="50dp"
        android:background="@drawable/edittext_background"
        android:ems="10"
        android:hint="Enter your Email"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColor="#FDFDFD"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="385dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="125dp"
        android:layout_height="22dp"
        android:fontFamily="@font/archivo_black"
        android:text="Set Password"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="452dp" />

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="343dp"
        android:layout_height="50dp"
        android:background="@drawable/edittext_background"
        android:ems="10"
        android:hint="8-Characters"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColor="#FDFDFD"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="474dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="161dp"
        android:layout_height="22dp"
        android:fontFamily="@font/archivo_black"
        android:text="Confirm Password"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="541dp" />

    <EditText
        android:id="@+id/editTextText7"
        android:layout_width="343dp"
        android:layout_height="50dp"
        android:background="@drawable/edittext_background"
        android:ems="10"
        android:hint="Re enter Password"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColor="#FDFDFD"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="563dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="343dp"
        android:layout_height="50dp"
        android:layout_marginBottom="28dp"
        android:background="#FFFFFF"
        android:text="Save Changes"
        app:backgroundTint="#24651F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>